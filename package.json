{
  "homepage": "https://github.com/marcusasplund/hyperapp-i18n-lab#readme",
  "license": "MIT",
  "main": "src/index.js",
  "name": "hyperapp-i18n-lab",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/marcusasplund/hyperapp-i18n-lab.git"
  },
  "scripts": {
    "start": "cp src/no.json dist && standard && parcel src/index.html -p 3000 --open",
    "build": "rm -rf release && standard && parcel build src/index.html -d build --public-url ./ && cp src/no.json build && workbox generateSW"
  },
  "version": "0.0.2",
  "author": "Marcus Asplund <marcus@greatname.se> (@marcusasplund)",
  "bugs": {
    "url": "https://github.com/marcusasplund/hyperapp-i18n-lab/issues"
  },
  "cache": {
    "inDev": true,
    "swDest": "service-worker.js"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "dist/",
      "build/"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "babel-eslint": "^10.0.3",
    "node-sass": "^4.9.3",
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-sw-cache": "^0.3.1",
    "standard": "*"
  },
  "dependencies": {
    "@hyperapp/events": "^0.0.3",
    "hyperapp": "^2.0.1",
    "hyperapp-fx": "^2.0.0-beta.1"
  }
}
